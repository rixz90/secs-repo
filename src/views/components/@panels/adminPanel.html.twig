<section>
	<h3>Administrator</h3>
	<div x-id="['input']">
		<label :for="$id('input')">Username:</label>
		<input type="text" name="id" :id="$id('input')" required />
	</div>

	<div x-id="['input']">
		<label :for="$id('input')">Department:</label>
		<input type="text" name="department" :id="$id('input')" required />
	</div>
	<div x-id="['input']">
		<label :for="$id('input')">Name:</label>
		<input type="text" name="name" :id="$id('input')" required />
	</div>
	<div x-id="['input']">
		<label :for="$id('input')">Password:</label>
		<input type="password" name="pass" autocomplete="new-password" :id="$id('input')" required />
	</div>
	<div x-id="['input']">
		<label :for="$id('input')">Conform password:</label>
		<input type="password" name="c_pass" autocomplete="new-password" :id="$id('input')" class="form-control"
			required />
	</div>
	<div style="text-align:center;">
		<input type="submit" style="width: fit-content;" />
		<input type="reset" value="Reset" style="width: fit-content;" />
	</div>
</section>